window.document.getElementById( "ID-CONTAINER" ).addEventListener( "click", function( e ){
	var t = e.target;
	if( t.tagName == "IMG" )
	{
		if( e.ctrlKey )
		{
			window.open( "http://thdtjsdn.shop/img/stock/charts/asfasfaf-fhd/" + t.getAttribute( "alt" ), "_blank" );
		}
		else
		{
			if( "hd" == t.getAttribute( "data-type" ) )
			{
				window.open( "http://thdtjsdn.shop/img/stock/charts/asfasfaf-hd/" + t.getAttribute( "alt" ), "_blank" );
			}
			else if( "fhd" == t.getAttribute( "data-type" ) )
			{
				window.open( "http://thdtjsdn.shop/img/stock/charts/asfasfaf-fhd/" + t.getAttribute( "alt" ), "_blank" );
			}
			else if( "uhd" == t.getAttribute( "data-type" ) )
			{
				window.open( "http://thdtjsdn.shop/img/stock/charts/asfasfaf-uhd/" + t.getAttribute( "alt" ), "_blank" );
			}
			else if( "fhd-hts" == t.getAttribute( "data-type" ) )
			{
				window.open( "http://thdtjsdn.shop/img/stock/charts-kiwoom/asfasfaf/" + t.getAttribute( "alt" ), "_blank" );
			}
			else if( "uhd-hts" == t.getAttribute( "data-type" ) )
			{
				window.open( "http://thdtjsdn.shop/img/stock/charts-kiwoom-uhd/asfasfaf/" + t.getAttribute( "alt" ), "_blank" );
			}
			else
			{
				window.open( "http://thdtjsdn.shop/img/stock/charts/asfasfaf-hd/" + t.getAttribute( "alt" ), "_blank" );
			}
		}
	}

	if( t.tagName == "INPUT" )
	{
		setTimeout( function(){
			window.__lazyLoad__IMG();
			window.__lazyLoad__NEWS();
			window.__lazyLoad__TORON();
		}, 200 );
	}

	if( t.tagName == "SPAN" )
	{
		if( t.getAttribute( "data-type" ) == "LINK" )
			window.open( "https://www.google.com/search?q=" + t.innerText, "_blank" );
	}
});

