		return "";
	};
	var TEMPLATE_TORON = '<span style="display: block; width: 100%; color: #fcba03">{{wid}} - {{dt}} - {{tt}}</span><span style="display: block; width: 100%;">{{ct}}</span><hr>';

	var timeout__lazyLoad__TORON;

	function lazyLoad__TORON (){
		if( timeout__lazyLoad__TORON ) window.clearTimeout( timeout__lazyLoad__TORON );

		var d = new Date();               var DATE_STR0 = getDateStr( d );
			d.setDate( d.getDate() - 1 ); var DATE_STR1 = getDateStr( d );
			d.setDate( d.getDate() - 1 ); var DATE_STR2 = getDateStr( d );
			d.setDate( d.getDate() - 1 ); var DATE_STR3 = getDateStr( d );
			d.setDate( d.getDate() - 1 ); var DATE_STR4 = getDateStr( d );
			//d.setDate( d.getDate() - 1 ); var DATE_STR5 = getDateStr( d );

		var els = window.document.querySelectorAll( "div.toro" );

		timeout__lazyLoad__TORON = setTimeout(function() {
			var scrollTop = window.document.getElementById( "ID-CONTAINER" ).scrollTop;
			//var scrollTop = window.pageYOffset;
			els.forEach(function( el ){

				if( el.parentElement.style.display == "none" || el.style.display == "none" ) return;

				if( el.offsetTop < ( window.document.getElementById( "ID-CONTAINER" ).offsetHeight + scrollTop )
					&& el.offsetTop > ( scrollTop - 700 ) )
				//if( el.offsetTop < ( window.innerHeight + scrollTop ) )
				{
					el.classList.remove( "toro" );

					var img = el.parentElement.parentElement.children[ 1 ].children[ 0 ];
					var name = img.getAttribute( "alt" ).replace( ".png", "" );


					var boards = [];
					try{ boards = boards.concat( JSON.parse( getTORONDate( DATE_STR0, name ) ) ); }catch( er ){}
					try{ boards = boards.concat( JSON.parse( getTORONDate( DATE_STR1, name ) ) ); }catch( er ){}
					try{ boards = boards.concat( JSON.parse( getTORONDate( DATE_STR2, name ) ) ); }catch( er ){}
					try{ boards = boards.concat( JSON.parse( getTORONDate( DATE_STR3, name ) ) ); }catch( er ){}
					try{ boards = boards.concat( JSON.parse( getTORONDate( DATE_STR4, name ) ) ); }catch( er ){}
					//try{ boards = boards.concat( JSON.parse( getTORONDate( DATE_STR5, name ) ) ); }catch( er ){}


					var HTML_INNER = "";
					boards.forEach( function( item0 ){
						HTML_INNER += TEMPLATE_TORON.replace( /{{wid}}/gi, item0.wid )
							.replace( /{{dt}}/gi, item0.dt )
							.replace( /{{tt}}/gi, item0.tt )
							.replace( /{{ct}}/gi, item0.ct )
					});
					el.innerHTML = HTML_INNER;
				}
			});

			if( els.length == 0 )
			{
				window.document.getElementById( "ID-CONTAINER" ).removeEventListener( "scroll", lazyLoad__TORON );
				//window.removeEventListener( "scroll", lazyLoad__TORON );
				window.removeEventListener( "resize", lazyLoad__TORON );
				window.removeEventListener( "orientationChange", lazyLoad__TORON );
			}
		}, 500 );
	}

	window.__lazyLoad__TORON = lazyLoad__TORON;

	window.document.getElementById( "ID-CONTAINER" ).addEventListener( "scroll", lazyLoad__TORON );
	//window.addEventListener( "scroll", lazyLoad__TORON );
	window.addEventListener( "resize", lazyLoad__TORON );
	//window.addEventListener( "orientationChange", lazyLoad__TORON );

	setTimeout( lazyLoad__TORON, 10 );

});

//----------------------------------------------------------------------------------------------------;

})();
</SCRIPT>
