window.document.addEventListener( "DOMContentLoaded", function(){
	var timeout__lazyLoad__IMG;

	function lazyLoad__IMG (){
		if( timeout__lazyLoad__IMG ) window.clearTimeout( timeout__lazyLoad__IMG );

		var els = window.document.querySelectorAll( "img.lazy" );

		timeout__lazyLoad__IMG = setTimeout(function() {
			var scrollTop = window.document.getElementById( "ID-CONTAINER" ).scrollTop;
			//var scrollTop = window.pageYOffset;
			els.forEach(function( el ){

				if( el.parentElement.style.display == "none" || el.style.display == "none" ) return;

				if( el.offsetTop < ( window.document.getElementById( "ID-CONTAINER" ).offsetHeight + scrollTop )
					&& el.offsetTop > ( scrollTop - 700 ) )
				//if( el.offsetTop < ( window.innerHeight + scrollTop ) )
				{
					el.classList.remove( "lazy" );
					el.onerror = function( e ){
						e.target.src = "https://via.placeholder.com/100/000.png";

						//*///로컬모드에서는 주석하기;
						e.target.style.display = "none";
						e.target.parentElement.style.display = "none";
						//*/
					};
					//img.removeAttribute( "data-src" );
					el.src = "http://thdtjsdn.shop/img/stock/" + el.dataset.src + "?t=" + Date.now();
				}
			});

			if( els.length == 0 )
			{
				window.document.getElementById( "ID-CONTAINER" ).removeEventListener( "scroll", lazyLoad__IMG );
				//window.removeEventListener( "scroll", lazyLoad__IMG );
				window.removeEventListener( "resize", lazyLoad__IMG );
				window.removeEventListener( "orientationChange", lazyLoad__IMG );
			}
		}, 200 );
	}
	window.__lazyLoad__IMG = lazyLoad__IMG;

	window.document.getElementById( "ID-CONTAINER" ).addEventListener( "scroll", lazyLoad__IMG );
	//window.addEventListener( "scroll", lazyLoad__IMG );
	window.addEventListener( "resize", lazyLoad__IMG );
	//window.addEventListener( "orientationChange", lazyLoad__IMG );

	setTimeout( lazyLoad__IMG, 10 );

	//--------------------------------------------------;
	//--------------------------------------------------;
	//--------------------------------------------------;
	//--------------------------------------------------;
	//--------------------------------------------------;

	var xhr = new XMLHttpRequest();
	var pad = function( n ){ var s = n.toString(); if( s.length == 1 ) return "0" + s; return s; };
	var getDateStr = function( d ){ return pad( d.getFullYear() ) + "-" + pad( d.getMonth() + 1 ) + "-" + pad( d.getDate() ); };
	var getNewsDate = function( DATE_STR, name ){
		try
		{
			xhr.open( "get", "http://thdtjsdn.shop/html/stock/" + DATE_STR + " - 종합시황뉴스 - 정제 - 종목 - 시간정렬 - " + window.apis.uri.getURIReplaceSharp( name ) + ".html", false );
			xhr.send();
			if( -1 == xhr.responseText.indexOf( "404 Not Found" ) ) return xhr.responseText;
		}catch( er ){}
		return "";
	};

	var timeout__lazyLoad__NEWS;

	function lazyLoad__NEWS (){
		if( timeout__lazyLoad__NEWS ) window.clearTimeout( timeout__lazyLoad__NEWS );

		var d = new Date();               var DATE_STR0 = getDateStr( d );
			d.setDate( d.getDate() - 1 ); var DATE_STR1 = getDateStr( d );
			d.setDate( d.getDate() - 1 ); var DATE_STR2 = getDateStr( d );
			d.setDate( d.getDate() - 1 ); var DATE_STR3 = getDateStr( d );
			d.setDate( d.getDate() - 1 ); var DATE_STR4 = getDateStr( d );
			//d.setDate( d.getDate() - 1 ); var DATE_STR5 = getDateStr( d );

		DATE_STR0 = DATE_STR0.replace( /-/gi, "" );
		DATE_STR1 = DATE_STR1.replace( /-/gi, "" );
		DATE_STR2 = DATE_STR2.replace( /-/gi, "" );
		DATE_STR3 = DATE_STR3.replace( /-/gi, "" );
		DATE_STR4 = DATE_STR4.replace( /-/gi, "" );
		//DATE_STR5 = DATE_STR5.replace( /-/gi, "" );

		timeout__lazyLoad__NEWS = setTimeout(function() {
			var els = window.document.querySelectorAll( "div.news" );

			var scrollTop = window.document.getElementById( "ID-CONTAINER" ).scrollTop;
			//var scrollTop = window.pageYOffset;
			els.forEach(function( el ){

				if( el.parentElement.style.display == "none" || el.style.display == "none" ) return;

				if( el.offsetTop < ( window.document.getElementById( "ID-CONTAINER" ).offsetHeight + scrollTop )
					&& el.offsetTop > ( scrollTop - 700 ) )
				//if( el.offsetTop < ( window.innerHeight + scrollTop ) )
				{
					el.classList.remove( "news" );

					var name = el.getAttribute( "alt" ).replace( ".png", "" );

					var HTML_INNER = "";
						HTML_INNER += getNewsDate( DATE_STR0, name );
						HTML_INNER += getNewsDate( DATE_STR1, name );
						HTML_INNER += getNewsDate( DATE_STR2, name );
						HTML_INNER += getNewsDate( DATE_STR3, name );
						HTML_INNER += getNewsDate( DATE_STR4, name );
						//HTML_INNER += getNewsDate( DATE_STR5, name );

					el.innerHTML = HTML_INNER;
				}
			});

			if( els.length == 0 )
			{
				window.document.getElementById( "ID-CONTAINER" ).removeEventListener( "scroll", lazyLoad__NEWS );
				//window.removeEventListener( "scroll", lazyLoad__NEWS );
				window.removeEventListener( "resize", lazyLoad__NEWS );
				window.removeEventListener( "orientationChange", lazyLoad__NEWS );
			}
		}, 200 );
	}
	window.__lazyLoad__NEWS = lazyLoad__NEWS;

	window.document.getElementById( "ID-CONTAINER" ).addEventListener( "scroll", lazyLoad__NEWS );
	//window.addEventListener( "scroll", lazyLoad__NEWS );
	window.addEventListener( "resize", lazyLoad__NEWS );
	//window.addEventListener( "orientationChange", lazyLoad__NEWS );

	setTimeout( lazyLoad__NEWS, 10 );

	//--------------------------------------------------;

	var getTORONDate = function( DATE_STR, name ){
		try
		{
			xhr.open( "get", "http://thdtjsdn.shop/download__json__toron?name=" + window.apis.uri.getURIReplaceSharp( name ) + "&date=" + DATE_STR, false );
			//xhr.open( "get", "http://localhost:49323/stock_naver_toron?name=" + window.apis.uri.getURIReplaceSharp( name ) + "&date=" + DATE_STR, false );
			xhr.send();
			if( -1 == xhr.responseText.indexOf( "404 Not Found" ) ) return xhr.responseText;
		}catch( er ){
			console.error( er );
		}
		return "";
	};
	var TEMPLATE_TORON = '<span style="display: block; width: 100%; color: #fcba03">{{wid}} - {{dt}} - {{tt}}</span><span style="display: block; width: 100%;">{{ct}}</span><hr>';

	var timeout__lazyLoad__TORON;

	function lazyLoad__TORON (){
		if( timeout__lazyLoad__TORON ) window.clearTimeout( timeout__lazyLoad__TORON );

		var d = new Date();               var DATE_STR0 = getDateStr( d );
			d.setDate( d.getDate() - 1 ); var DATE_STR1 = getDateStr( d );
			d.setDate( d.getDate() - 1 ); var DATE_STR2 = getDateStr( d );
			d.setDate( d.getDate() - 1 ); var DATE_STR3 = getDateStr( d );
			d.setDate( d.getDate() - 1 ); var DATE_STR4 = getDateStr( d );
			//d.setDate( d.getDate() - 1 ); var DATE_STR5 = getDateStr( d );

		timeout__lazyLoad__TORON = setTimeout(function() {
			var els = window.document.querySelectorAll( "div.toro" );

			var scrollTop = window.document.getElementById( "ID-CONTAINER" ).scrollTop;
			//var scrollTop = window.pageYOffset;
			els.forEach(function( el ){

				if( el.parentElement.style.display == "none" || el.style.display == "none" ) return;

				if( el.offsetTop < ( window.document.getElementById( "ID-CONTAINER" ).offsetHeight + scrollTop )
					&& el.offsetTop > ( scrollTop - 700 ) )
				//if( el.offsetTop < ( window.innerHeight + scrollTop ) )
				{
					el.classList.remove( "toro" );

					var name = el.getAttribute( "alt" ).replace( ".png", "" );

					var boards = [];
					try{ boards = boards.concat( JSON.parse( getTORONDate( DATE_STR0, name ) ) ); }catch( er ){}
					try{ boards = boards.concat( JSON.parse( getTORONDate( DATE_STR1, name ) ) ); }catch( er ){}
					try{ boards = boards.concat( JSON.parse( getTORONDate( DATE_STR2, name ) ) ); }catch( er ){}
					try{ boards = boards.concat( JSON.parse( getTORONDate( DATE_STR3, name ) ) ); }catch( er ){}
					try{ boards = boards.concat( JSON.parse( getTORONDate( DATE_STR4, name ) ) ); }catch( er ){}
					//try{ boards = boards.concat( JSON.parse( getTORONDate( DATE_STR5, name ) ) ); }catch( er ){}


					var HTML_INNER = "";
					boards.forEach( function( item0 ){
						HTML_INNER += TEMPLATE_TORON.replace( /{{wid}}/gi, item0.wid )
							.replace( /{{dt}}/gi, item0.dt )
							.replace( /{{tt}}/gi, item0.tt )
							.replace( /{{ct}}/gi, item0.ct )
					});
					el.innerHTML = HTML_INNER;
				}
			});

			if( els.length == 0 )
			{
				window.document.getElementById( "ID-CONTAINER" ).removeEventListener( "scroll", lazyLoad__TORON );
				//window.removeEventListener( "scroll", lazyLoad__TORON );
				window.removeEventListener( "resize", lazyLoad__TORON );
				window.removeEventListener( "orientationChange", lazyLoad__TORON );
			}
		}, 200 );
	}

	window.__lazyLoad__TORON = lazyLoad__TORON;

	window.document.getElementById( "ID-CONTAINER" ).addEventListener( "scroll", lazyLoad__TORON );
	//window.addEventListener( "scroll", lazyLoad__TORON );
	window.addEventListener( "resize", lazyLoad__TORON );
	//window.addEventListener( "orientationChange", lazyLoad__TORON );

	setTimeout( lazyLoad__TORON, 10 );

	(function(e){
		window.__API.hide.all();

		//window.__API.show[ "IMG-MINI" ]();

		//window.__API.show[ 'IMG-BASIC' ]();
		window.__API.show[ 'IMG-HD' ]();
		//window.__API.show.news();
		//window.__API.show.toro();
	})();
});

//----------------------------------------------------------------------------------------------------;

})();
